{{!< main}}
<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>


<form class="ui form" id="form" action="/design/add" method="POST" enctype="multipart/form-data">
  <div part="1" class="one part" hidden>
    {{> design/one}}
  </div>

  <div part="2" class="two part" hidden>
    {{> design/two}}
  </div>

  <div part="3" class="three part">
    {{> design/three}}
  </div>

  <div part="4" class="four part" >
    {{> design/four}}
  </div>

  <div part="5" class="five part" hidden>
    {{> design/five}}
  </div>

  <div part="6" class="six part" hidden>
    {{> design/six}}
  </div>

  <div part="7" class="seven part" hidden>
    {{> design/seven}}
  </div>


</form>
<br><br>

{{!-- <button class="ui inverted fluid red large button" onclick="event.preventDefault();">등록하기</button> --}}

<br><br>

<script>
  function next(part) {
    if (part === 7) {
      alert('end!')
      return false
    }
    $('div.part').hide();
    $(`[part=${part + 1}]`).show();
  }
</script>